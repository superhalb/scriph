<!DOCTYPE html>
<html>
	<head>
    {{! Document Settings }}
    <meta http-equiv="Content-Type" content="text/html" charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    {{! Responsive Meta Tags }}
    <meta name="HandheldFriendly" content="True" />
    <meta name="MobileOptimized" content="320" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Scriph</title>
		{{>assets}}
		<link href="css/settings.css" rel="stylesheet" type="text/css">
		<link href="css/post-settings.css" rel="stylesheet" type="text/css">
	</head>
  <body>
		<div id="systemindex">
			{{>header}}
			<div id="settings">
				<form class="section" id="blog">
					<div class="section-name">Blog</div>
					<div class="section-values">
						{{# Blog}}
							{{>entity}}
						{{/ Blog}}
					</div>
				</form>
				<form class="section" id="theme">
					<div class="section-name">Tema</div>
					<div class="section-values">
					{{# Theme}}
						{{>entity}}
					{{/ Theme}}
					</div>
				</form>
			</div>
		</div>
		{{> jsvendors }}
		<script src="js/settings.menu.js" type="text/javascript"></script>
		<script src="js/common.menu.js" type="text/javascript"></script>
  </body>
</html>